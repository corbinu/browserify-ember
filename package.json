{
	"name": "browserify-ember",
	"version": "0.0.1",
	"description": "Browserify Emberjs",
	"dependencies": {
		"browserify-shim": "~3.2.2"
	},
	"main": "./main.js",
	"browser": {
		"jquery": "./Vendor/jquery/jquery.js",
		"handlebars": "./Vendor/handlebars/handlebars.js",
		"ember": "./Vendor/ember/ember.js"
	},
	"browserify-shim": {
		"jquery": "$",
		"ember": { "depends": [ "jquery:jquery", "handlebars:handlebars" ] }
	},
	"browserify": {
		"transform": [ "browserify-shim" ]
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": ""
	},
	"author": "Corbin Uselton",
	"license": "MIT"
}
